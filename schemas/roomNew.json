{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/example.json",
  "type": "object",
  "properties": {
    "room_name": {
      "type": "string",
      "minLength": 1,
      "maxLength": 30
    },
    "password": {
      "type": "string",
      "minLength": 1,
      "maxLength": 30,
      "pattern": "^[^\\s]*$"
    }
  },
  "additionalProperties": false,
  "required": [
    "room_name"
  ]
}
